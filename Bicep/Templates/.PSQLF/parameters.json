{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "DepartmentCode": {
            "value": "#{teamUniqueCode}",
            "metadata": {
                "description": "The team or department unique code"
        }
        },
        "ApplicationName": {
            "value": "#{applicationName}",
            "metadata": {
                "description": "The name of the application that the resource is deployed for"
            }
        },
        "AppEnvironment": {
            "value": "#{applicationEnvironment}",
            "metadata": {
                "description": "The environment of the application where the resource is deployed"
            }
        },
        "LocationCode": {
            "value": "#{LocationCode}",
            "metadata": {
                "description": "The Location Code of the application where the resource is deployed"
            }
        },
        "FreeTextForName": {
            "value": "#{Private.PSQLF.FreeTextForName}",
            "metadata": {
                "description": "Name of the application"
            }
        },
        "virtualNetworkId": {
            "value": "#{Private.VNET}",
            "metadata": {
                "description": "Virtual Network resourceId to integrate the application"
            }
        },
        "delegatedSubnetName": {
            "value": "#{Private.PSQLF.delegatedSubnetName}",
            "metadata": {
                "description": "Specify Delegated Subnet Arguments"
            }
        },
        "DelegatedSubnetAddressPrefix": {
            "value": "#{Private.PSQLF.DelegatedSubnetAddressPrefix}",
            "metadata": {
                "description": "Specify AddressPrefix associated to subnet of virtualNetwork"
            }
        },
        "DelegatedSubnetNSGId": {
            "value": "#{Private.PSQLF.DelegatedSubnetNSGId}",
            "metadata": {
                "description": "Specify networkSecurityGroup Id associated with subnet of virtualNetwork"
            }
        },
        "administratorLogin": {
            "value": "#{Private.PSQLF.administratorLogin}",
            "metadata": {
                "description": "The Administrator Login of the application"
            }
        },
        "administratorLoginPassword": {
            "value": "#{Private.PSQLF.administratorLoginPassword}",
            "metadata": {
                "description": "The Administrator Login Password of the application"
            }
        },
        "version": {
            "value": "#{Private.PSQLF.version}",
            "metadata": {
                "description": "Version of the application"
            }
        }
    }
}